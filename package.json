  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MajAhd/leouser_api.git"
  },
  "keywords": [
    "LeoVegas"
  ],
  "author": "Majid",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MajAhd/leouser_api/issues"
  },
  "homepage": "https://github.com/MajAhd/leouser_api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@elastic/elasticsearch": "^8.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "elasticsearch": "^16.7.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  }
